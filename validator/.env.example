# PixCrawler Validator Package Configuration
# Copy this file to .env and adjust values as needed

# Validation behavior
PIXCRAWLER_VALIDATOR_MODE=LENIENT
PIXCRAWLER_VALIDATOR_DUPLICATE_ACTION=REMOVE

# File constraints
PIXCRAWLER_VALIDATOR_MAX_FILE_SIZE_MB=10
PIXCRAWLER_VALIDATOR_MIN_FILE_SIZE_BYTES=1024
