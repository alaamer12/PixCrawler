# PixCrawler Builder Package Configuration
# Copy this file to .env and adjust values as needed

# Configuration files
PIXCRAWLER_BUILDER_CONFIG_PATH=config.json
PIXCRAWLER_BUILDER_CACHE_FILE=download_progress.json

# Processing limits
PIXCRAWLER_BUILDER_MAX_IMAGES=10
PIXCRAWLER_BUILDER_MAX_RETRIES=5

# Dataset configuration
PIXCRAWLER_BUILDER_OUTPUT_DIR=
PIXCRAWLER_BUILDER_DATASET_NAME=
PIXCRAWLER_BUILDER_CONTINUE_FROM_LAST=false

# AI and keyword generation
PIXCRAWLER_BUILDER_KEYWORD_GENERATION=auto
PIXCRAWLER_BUILDER_AI_MODEL=gpt4-mini

# Feature flags
PIXCRAWLER_BUILDER_GENERATE_LABELS=true

# Search variations (JSON array format)
PIXCRAWLER_BUILDER_SEARCH_VARIATIONS=